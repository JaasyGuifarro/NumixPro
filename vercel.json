{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "outputDirectory": "NumixPro-main/.next",
  "buildCommand": "cd NumixPro-main && npm install --legacy-peer-deps && npm run build",
  "installCommand": "cd NumixPro-main && npm install --legacy-peer-deps",
  "framework": "nextjs"
}